<?xml version="1.0" ?>
<mujoco model="panda_tray_cylinder">
  <compiler angle="radian" coordinate="local" meshdir="." texturedir="."/>
  <option timestep="0.002" gravity="0 0 -9.81"/>
  
  <default>
    <joint damping="1" limited="true"/>
    <geom contype="1" conaffinity="1" condim="3"/>
    <motor ctrlrange="-1 1" ctrllimited="true"/>
  </default>

  <!-- MATERIALS -->
  <asset>
    <material name="panda_white" rgba="1 1 1 1"/>  
    <material name="panda_gray" rgba="0.8 0.8 0.8 1"/>
    <material name="tray_black" rgba="0.4 0.4 0.4 1"/>
    <material name="tray_rim"  rgba="0.4 0.4 0.4 1"/>
    <material name="ball_red" rgba="1 0 0 1"/>
    <material name="environment" rgba="0.8 0.8 0.8 1"/>
    
    <!-- Panda link meshes -->
    <mesh name="link0" file="link0.stl"/>
    <mesh name="link1" file="link1.stl"/>
    <mesh name="link2" file="link2.stl"/>
    <mesh name="link3" file="link3.stl"/>
    <mesh name="link4" file="link4.stl"/>
    <mesh name="link5" file="link5.stl"/>
    <mesh name="link6" file="link6.stl"/>
    <mesh name="link7" file="link7.stl"/>
  </asset>

  <worldbody>
    <!-- Panda Robot -->
    <body name="panda_link0" pos="0 0 0">
      <geom type="mesh" mesh="link0" material="panda_gray"/>
      
      <body name="panda_link1" pos="0 0 0.333">
        <joint name="panda_joint1" type="hinge" pos="0 0 0" axis="0 0 1" range="-2.8973 2.8973" damping="1.0"/>
        <geom type="mesh" mesh="link1" material="panda_white"/>
        
        <body name="panda_link2" pos="0 0 0" quat="0.7071068 -0.7071068 0 0">
          <joint name="panda_joint2" type="hinge" axis="0 0 1" range="-1.7628 1.7628"/>
          <geom type="mesh" mesh="link2" material="panda_white"/>
          
          <body name="panda_link3" pos="0 -0.316 0" quat="0.7071068 0.7071068 0 0">
            <joint name="panda_joint3" type="hinge" axis="0 0 1" range="-2.8973 2.8973"/>
            <geom type="mesh" mesh="link3" material="panda_white"/>
            
            <body name="panda_link4" pos="0.0825 0 0" quat="0.7071068 0.7071068 0 0">
              <joint name="panda_joint4" type="hinge" axis="0 0 1" range="-3.0718 -0.0698"/>
              <geom type="mesh" mesh="link4" material="panda_white"/>
              
              <body name="panda_link5" pos="-0.0825 0.384 0" quat="0.7071068 -0.7071068 0 0">
                <joint name="panda_joint5" type="hinge" axis="0 0 1" range="-2.8973 2.8973"/>
                <geom type="mesh" mesh="link5" material="panda_white"/>
                
                <body name="panda_link6" pos="0 0 0" quat="0.7071068 0.7071068 0 0">
                  <joint name="panda_joint6" type="hinge" axis="0 0 1" range="-0.0175 3.7525"/>
                  <geom type="mesh" mesh="link6" material="panda_white"/>
                  
                  <body name="panda_link7" pos="0.088 0 0" quat="0.7071068 0.7071068 0 0">
                    <joint name="panda_joint7" type="hinge" axis="0 0 1" range="-2.8973 2.8973"/>
                    <geom type="mesh" mesh="link7" material="panda_white"/>
                    
                    <!-- End Effector -->
                    <body name="panda_EndEffector" pos="0 0 0.107" quat="0.9238795 0 0 0.3826834">
                      
                      <!-- Tray attached to end effector -->
                      <body name="tray_base" pos="0 0 0.09" quat="0.7071068 0 0.7071068 0">
                        
                        <!-- Tray base -->
                        <geom name="tray_base_geom" type="box" size="0.08 0.12 0.005" material="tray_black"/>
                        
                        <!-- Tray rims -->
                        <geom name="tray_rim_xpos" type="box" size="0.01 0.12 0.005" pos="0.08 0 0.01" material="tray_rim"/>
                        <geom name="tray_rim_xneg" type="box" size="0.01 0.12 0.005" pos="-0.08 0 0.01" material="tray_rim"/>
                        <geom name="tray_rim_ypos" type="box" size="0.08 0.01 0.005" pos="0 0.12 0.01" material="tray_rim"/>
                        <geom name="tray_rim_yneg" type="box" size="0.08 0.01 0.005" pos="0 -0.12 0.01" material="tray_rim"/>
                      </body>
                    </body>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>

    <!-- Cylinder object -->
    <body name="cylinder" pos="0.5 0.3 0.68">
      <freejoint name="cylinder_free"/>
      <geom name="cylinder_geom" type="cylinder" size="0.02 0.03" material="ball_red"/>
    </body>

    <!-- Ground plane -->
    <geom name="ground" type="plane" size="2 2 0.1" pos="0 0 0" material="environment"/>
  </worldbody>

  <actuator>
    <motor name="panda_joint1_motor" joint="panda_joint1" gear="87"/>
    <motor name="panda_joint2_motor" joint="panda_joint2" gear="87"/>
    <motor name="panda_joint3_motor" joint="panda_joint3" gear="87"/>
    <motor name="panda_joint4_motor" joint="panda_joint4" gear="87"/>
    <motor name="panda_joint5_motor" joint="panda_joint5" gear="12"/>
    <motor name="panda_joint6_motor" joint="panda_joint6" gear="12"/>
    <motor name="panda_joint7_motor" joint="panda_joint7" gear="12"/>
  </actuator>
</mujoco>